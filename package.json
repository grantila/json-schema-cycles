{
  "name": "json-schema-cycles",
  "version": "0.0.0-development",
  "description": "Analyize recursive (cyclic) JSON Schema types",
  "author": "Gustaf Räntilä",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/grantila/json-schema-cycles/issues"
  },
  "homepage": "https://github.com/grantila/json-schema-cycles#readme",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.prod.json",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "cz": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/grantila/json-schema-cycles"
  },
  "keywords": [
    "json",
    "schema",
    "jsonschema",
    "cycles",
    "cyclic",
    "recursive"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "cz-conventional-changelog": "^3.1.0",
    "jest": "^27.2.0",
    "rimraf": "^3.0.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "graph-cycles": "^2.0.0",
    "json-schema-traverse": "^1.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ],
    "extensionsToTreatAsEsm": [
      ".ts"
    ]
  }
}
